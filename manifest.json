{
    "name" : "MEWA",
    "version": "1.2",
    "manifest_version": 2,
    "icons": {"128": "icon_128.png"},
    "content_scripts": [
        {
            "matches": ["https://prod.lms.macmillaneducation.com/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_title": "MEWA",
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "https://prod.lms.macmillaneducation.com/*"
    ]
}